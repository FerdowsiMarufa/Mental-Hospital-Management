         <rowConstraints>
            <RowConstraints maxHeight="55.0" minHeight="10.0" prefHeight="55.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="51.0" minHeight="10.0" prefHeight="38.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <Label alignment="CENTER" prefHeight="31.0" prefWidth="80.0" text="ID">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <Label fx:id="passwordLabel" alignment="CENTER" prefHeight="41.0" prefWidth="85.0" text="Password" GridPane.rowIndex="1">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <TextField fx:id="idField" prefHeight="35.0" prefWidth="276.0" GridPane.columnIndex="1" />
            <PasswordField fx:id="passwordField" prefHeight="37.0" prefWidth="276.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
