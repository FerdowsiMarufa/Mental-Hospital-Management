<AnchorPane prefHeight="507.0" prefWidth="746.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bd.edu.seu.HomePageController">
   <children>
      <ImageView fitHeight="463.0" fitWidth="781.0" layoutX="-11.0" layoutY="96.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@unnamed.jpg" />
         </image>
      </ImageView>
      <Label alignment="CENTER" layoutX="7.0" layoutY="7.0" prefHeight="67.0" prefWidth="763.0" text="Circle Mental Hospital" textFill="#290936ba">
         <font>
            <Font size="58.0" />
         </font>
      </Label>
      <MenuButton alignment="CENTER" layoutX="480.0" layoutY="254.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="107.0" text="Login">
         <items>
            <MenuItem mnemonicParsing="false" onAction="#AdminLoginAction" text="Admin" />
            <MenuItem mnemonicParsing="false" onAction="#RECLoginAction" text="Receptionist" />
            <MenuItem mnemonicParsing="false" onAction="#DOCLoginAction" text="Doctor" />
         </items>
         <font>
            <Font size="19.0" />
         </font>
      </MenuButton>
      <GridPane layoutX="300.0" layoutY="150.0" prefHeight="95.0" prefWidth="338.0">
